class Complete{
    
   
   
    public static int multiply (int arr[], int n) {
     int sum1=0;
     int sum2=0;
     for(int i=0;i<n/2;i++)
     {
         sum1=sum1+arr[i];
     }
     for(int j=n/2;j<n;j++)
     {
     sum2=sum2+arr[j];
     }
     int mul=sum1*sum2;
     return mul;
     
      
    }
}
